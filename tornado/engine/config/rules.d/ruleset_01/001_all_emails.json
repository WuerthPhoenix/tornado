{
    "name": "all_emails",
    "description": "This matches all emails",
    "continue": true,
    "active": true,
    "constraint": {
      "WHERE": {
        "type": "AND",
        "operators": [
          {
            "type": "equal",
            "first": "${event.type}",
            "second": "email"
          }
        ]
      },
      "WITH": {}
    },
    "actions": [
      {
        "id": "Logger",
        "payload": {
          "type": "${event.type}",
          "subject": "${event.payload.subject}"
        }
    }
    ]
  }
