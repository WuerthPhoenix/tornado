{
  "name": "icinga_process_check_result",
  "description": "Set a process status in Icinga2",
  "continue": true,
  "active": true,
  "constraint": {
    "WHERE": {
      "type": "AND",
      "operators": []
    },
    "WITH": {}
  },
  "actions": [
    {
      "id": "icinga2",
