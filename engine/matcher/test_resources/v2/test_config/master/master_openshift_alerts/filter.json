{
  "type": "filter",
  "name": "master_openshift_alerts",
  "description": "This filter allows events for master tenant",
  "active": true,
  "filter": {
    "type": "AND",
    "operators": [
      {
        "type": "equals",
        "first": "${event.type}",
        "second": "openshift-alerts"
      }
    ]
  }
}
